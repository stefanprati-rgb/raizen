{
    "modelo_identificado": "Termo de Adesão Raízen Power / Consórcio RZ 9p",
    "distribuidora_principal": "CPFL_PAULISTA",
    "paginas_analisadas": 9,
    "campos": {
        "razao_social": {
            "regex": "Razão\\s+Social:[\\s\\n]*([^\\n\\-]+)",
            "grupo": 1,
            "descricao": "Nome da empresa ou cliente (Consorciada)"
        },
        "cnpj": {
            "regex": "CNPJ[\\s\\n]*(?:nº|MF[\\s\\n]+sob[\\s\\n]+o[\\s\\n]+nº)?[\\s\\n]*(\\d{2}\\.\\d{3}\\.\\d{3}/\\d{4}-\\d{2})",
            "grupo": 1,
            "descricao": "CNPJ do cliente formatado"
        },
        "num_instalacao": {
            "regex": "(?:Nº\\s+da\\s+Instalação\\s+(?:\\(Unidade\\s+Consumidora\\):|da\\s+Unidade\\s+Consumidora:))[\\s\\n]*(\\d+)",
            "grupo": 1,
            "descricao": "Número da Unidade Consumidora (UC)"
        },
        "num_cliente": {
            "regex": "Nº\\s+do\\s+Cliente:[\\s\\n]*(\\d+)",
            "grupo": 1,
            "descricao": "Número do código do cliente na distribuidora"
        },
        "distribuidora": {
            "regex": "Distribuidora:[\\s\\n]*(.*)",
            "grupo": 1,
            "descricao": "Nome da concessionária de energia"
        },
        "duracao_meses": {
            "regex": "(?:Duração:|Vigência\\s+Inicial:)[\\s\\n]*(\\d+)[\\s\\n]*meses",
            "grupo": 1,
            "descricao": "Prazo de validade do contrato em meses"
        },
        "email": {
            "regex": "E-mail:[\\s\\n]*([\\w\\.\\-]+@[\\w\\.\\-]+\\.[a-zA-Z]{2,})",
            "grupo": 1,
            "descricao": "Endereço de e-mail de contato da consorciada"
        },
        "representante_nome": {
            "regex": "(?:Nome:|Representante[\\s\\n]+Legal:)[\\s\\n]*([A-ZÀ-Ú\\s]+)(?=[\\s\\n]*[\\-\\n]|CPF)",
            "grupo": 1,
            "descricao": "Nome completo do representante legal ou titular"
        },
        "endereco": {
            "regex": "Endereço:[\\s\\n]*(.*?)(?=[\\s\\n]*(?:CEP:|Titular|CONSÓRCIO|$))",
            "grupo": 1,
            "descricao": "Endereço completo da sede ou da instalação"
        },
        "telefone": {
            "regex": "Telefone:[\\s\\n]*(\\(?\\d{2}\\)?[\\s\\n]*\\d{4,5}[\\-\\s]?\\d{4})",
            "grupo": 1,
            "descricao": "Telefone de contato"
        },
        "desconto_percentual": {
            "regex": "(?:Desconto|Economia)[\\s\\n]*de[\\s\\n]*(\\d+(?:[,\\.]\\d+)?)\\s*%",
            "grupo": 1,
            "descricao": "Percentual de desconto ou economia garantida"
        },
        "data_assinatura": {
            "regex": "(?:Piracicaba,)[\\s\\n]*(\\d{2}\\s+de\\s+\\w+\\s+(?:de\\s+)?\\d{4})",
            "grupo": 1,
            "descricao": "Data de celebração do termo encontrada no rodapé ou encerramento"
        }
    }
}